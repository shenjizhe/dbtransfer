<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ninehcom.transfer.mapper.CoachHistoryDataMapper">
    <resultMap type="com.ninehcom.transfer.entity.CoachHistoryData" id="coachHistoryDataMap">
        <id column="Id" property="id"/>
        <result column="TeamId" property="teamId"/>
        <result column="TeamHistoryId" property="teamHistoryId"/>
        <result column="TeamLogo" property="teamLogo"/>
        <result column="TeamName" property="teamName"/>
        <result column="CoachId" property="coachId"/>
        <result column="HalfLogo" property="halfLogo"/>
        <result column="InfoLogo" property="infoLogo"/>
        <result column="Logo" property="logo"/>
        <result column="CoachName" property="coachName"/>
        <result column="EnglishName" property="englishName"/>
        <result column="Age" property="age"/>
        <result column="CoachTypeId" property="coachTypeId"/>
        <result column="CoachTypeName" property="coachTypeName"/>
        <result column="Years" property="years"/>
        <result column="CreateTime" property="createTime"/>
        <result column="UpdateTime" property="updateTime"/>
    </resultMap>
    <select id="selectAllCoachHistoryData" resultMap="coachHistoryDataMap">
        SELECT
        *
        FROM
        coachhistory
    </select>
    <select id="selectCoachHistoryDataById" resultMap="coachHistoryDataMap">
        SELECT
        *
        FROM
        coachhistory
        WHERE
        ID=#{0}
    </select>
</mapper>

